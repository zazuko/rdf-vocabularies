{
  "name": "@vocabulary/stat",
  "version": "1.0.5",
  "type": "module",
  "description": "Describes terms for POSIX-like files and directory listings",
  "scripts": {
    "fetch": "npx vocab-build datasets",
    "prebuild": "npx vocab-build module",
    "build": "tsc",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@vocabulary/builder": "^1.0.10"
  },
  "homepage": "https://prefix.zazuko.com/prefix/stat:",
  "vocabulary": {
    "prefix": "stat",
    "namespace": "http://www.w3.org/ns/posix/stat#",
    "xmlParserOptions": {
      "baseIRI": "http://www.w3.org/ns/posix/stat#"
    }
  },
  "bugs": {
    "url": "https://github.com/zazuko/rdf-vocabularies/issues"
  },
  "license": "MIT",
  "files": [
    "*.js",
    "*.d.ts",
    "*.nq",
    "*.nt"
  ],
  "peerDependencies": {
    "@rdfjs/types": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zazuko/rdf-vocabularies",
    "directory": "ontologies/stat"
  }
}
