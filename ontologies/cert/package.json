{
  "name": "@vocabulary/cert",
  "version": "1.0.4",
  "type": "module",
  "description": "Ontology for Certificates and crypto stuff",
  "scripts": {
    "fetch": "npx --node-options='--loader=ts-node/esm' vocab-build datasets",
    "prebuild": "npx --node-options='--loader=ts-node/esm' vocab-build module",
    "build": "tsc",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@vocabulary/builder": "^1.0.8"
  },
  "license": "MIT",
  "homepage": "https://prefix.zazuko.com/prefix/cert:",
  "vocabulary": {
    "prefix": "cert",
    "namespace": "http://www.w3.org/ns/auth/cert#"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "*.nq",
    "*.nt"
  ],
  "bugs": {
    "url": "https://github.com/zazuko/rdf-vocabularies/issues"
  },
  "peerDependencies": {
    "@rdfjs/types": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zazuko/rdf-vocabularies",
    "directory": "ontologies/cert"
  }
}
