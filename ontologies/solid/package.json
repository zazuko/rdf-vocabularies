{
  "name": "@vocabulary/solid",
  "version": "1.0.0",
  "type": "module",
  "description": "The Solid Terms vocabulary defines terms referenced in Solid specifications",
  "scripts": {
    "fetch": "npx --node-options='--loader=ts-node/esm' vocab-build datasets",
    "prebuild": "npx --node-options='--loader=ts-node/esm' vocab-build module",
    "build": "tsc",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@vocabulary/builder": "^1.0.1"
  },
  "homepage": "https://prefix.zazuko.com/prefix/solid:",
  "vocabulary": {
    "prefix": "solid",
    "namespace": "http://www.w3.org/ns/solid/terms#"
  },
  "bugs": {
    "url": "https://github.com/zazuko/rdf-vocabularies/issues"
  },
  "license": "MIT",
  "files": [
    "*.js",
    "*.d.ts",
    "*.nq",
    "*.nt"
  ]
}
