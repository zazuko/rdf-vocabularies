{
  "name": "@zazuko/rdf-vocabularies",
  "version": "2019.12.23",
  "description": "Access most commonly used RDF ontologies/schemas/vocabularies as datasets or n-quads",
  "main": "lib/node/index.js",
  "module": "lib/es/index.js",
  "files": [
    "lib",
    "ontologies",
    "*.d.ts",
    "*.d.ts.map"
  ],
  "scripts": {
    "fetch": "npm run build; ts-node fetch.ts",
    "lint": "eslint . --ext .js,.ts",
    "pretest": "npm run lint",
    "test": "jest",
    "build:node": "tsc --target es2017 --module commonjs --outDir lib/node",
    "build:modern": "tsc --outDir lib/es",
    "build:types": "tsc --outDir . --declaration --declarationMap --emitDeclarationOnly",
    "build:es-loader": "tsc src/es/loadDataset.ts --target esnext --module esnext --esModuleInterop --moduleResolution node --outDir lib/es",
    "build": "rimraf lib; npm run build:node; npm run build:modern; npm run build:es-loader; npm run build:types",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zazuko/rdf-vocabularies.git"
  },
  "author": "Victor Felder <victor@draft.li> (https://draft.li)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zazuko/rdf-vocabularies/issues"
  },
  "homepage": "https://github.com/zazuko/rdf-vocabularies#readme",
  "devDependencies": {
    "@rdfjs/fetch-lite": "^2.0.0",
    "@rdfjs/formats-common": "^2.0.0",
    "@types/jest": "^24.0.19",
    "@types/rdf-ext": "^1.3.2",
    "@types/rdf-js": "^2.0.8",
    "@types/rdfjs__fetch-lite": "^2.0.0",
    "@types/rdfjs__formats-common": "^2.0.0",
    "@types/rdfjs__parser-n3": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "coveralls": "^3.0.7",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "rdfxml-streaming-parser": "^1.3.3",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@rdfjs/parser-n3": "^1.1.2",
    "rdf-ext": "^1.3.0",
    "string-to-stream": "^3.0.1"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "prefix",
    "namespace",
    "ontologies",
    "vocabularies"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true
  }
}
