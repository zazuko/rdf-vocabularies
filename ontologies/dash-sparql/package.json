{
  "name": "@vocabulary/dash-sparql",
  "version": "1.0.1",
  "type": "module",
  "main": "index.js",
  "description": "SPARQL Vocabulary for SHACL",
  "scripts": {
    "fetch": "npx --node-options='--loader=ts-node/esm' vocab-build datasets",
    "prebuild": "npx --node-options='--loader=ts-node/esm' vocab-build module",
    "build": "tsc",
    "prepack": "npm run build"
  },
  "homepage": "https://prefix.zazuko.com/prefix/dash-sparql:",
  "devDependencies": {
    "@vocabulary/builder": "^1.0.2"
  },
  "vocabulary": {
    "prefix": "dash-sparql",
    "namespace": "http://datashapes.org/sparql#"
  },
  "license": "MIT",
  "files": [
    "*.js",
    "*.d.ts",
    "*.nq",
    "*.nt"
  ]
}
