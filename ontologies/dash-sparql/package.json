  "files": [
    "*.js",
    "*.d.ts",
    "*.nq",
    "*.nt"
  ],
  "bugs": {
    "url": "https://github.com/zazuko/rdf-vocabularies/issues"
  },
  "peerDependencies": {
    "@rdfjs/types": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zazuko/rdf-vocabularies",
    "directory": "ontologies/dash-sparql"
  }
