{
  "name": "@vocabulary/gml",
  "version": "1.0.5",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "fetch": "npx --node-options='--loader=ts-node/esm' vocab-build datasets",
    "prebuild": "npx --node-options='--loader=ts-node/esm' vocab-build module",
    "build": "tsc",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@vocabulary/builder": "^1.0.8"
  },
  "homepage": "https://prefix.zazuko.com/prefix/gml:",
  "vocabulary": {
    "prefix": "gml",
    "namespace": "http://www.opengis.net/ont/gml#",
    "file": "http://schemas.opengis.net/gml/3.2.1/gml_32_geometries.rdf",
    "mediaType": "application/rdf+xml"
  },
  "bugs": {
    "url": "https://github.com/zazuko/rdf-vocabularies/issues"
  },
  "license": "MIT",
  "files": [
    "*.js",
    "*.d.ts",
    "*.nq",
    "*.nt"
  ],
  "peerDependencies": {
    "@rdfjs/types": "^2.0.0"
  }
}
