{
  "name": "@vocabulary/m4i",
  "version": "1.2.0",
  "type": "module",
  "description": "Metadata4Ing: An ontology for describing the generation of research data within a scientific activity.",
  "scripts": {
    "fetch": "npx --node-options='--loader=ts-node/esm' vocab-build datasets",
    "prebuild": "npx --node-options='--loader=ts-node/esm' vocab-build module",
    "build": "tsc",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@vocabulary/builder": "^1.0.7"
  },
  "homepage": "https://prefix.zazuko.com/prefix/m4i:",
  "vocabulary": {
    "prefix": "m4i",
    "namespace": "http://w3id.org/nfdi4ing/metadata4ing#"
  },
  "bugs": {
    "url": "https://github.com/zazuko/rdf-vocabularies/issues"
  },
  "license": "MIT",
  "files": [
    "*.js",
    "*.d.ts",
    "*.nq",
    "*.nt"
  ],
  "peerDependencies": {
    "@rdfjs/types": "^1.0.0"
  }
}
