{
  "name": "@vocabulary/rico",
  "version": "2.0.1",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "fetch": "npx --node-options='--loader=ts-node/esm' vocab-build datasets",
    "prebuild": "npx --node-options='--loader=ts-node/esm' vocab-build module",
    "build": "tsc",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@vocabulary/builder": "^1.0.8"
  },
  "homepage": "https://prefix.zazuko.com/prefix/rico:",
  "vocabulary": {
    "prefix": "rico",
    "namespace": "https://www.ica.org/standards/RiC/ontology#",
    "file": "https://github.com/ICA-EGAD/RiC-O/raw/master/ontology/current-version/RiC-O_1-0-2.rdf",
    "mediaType": "application/rdf+xml"
  },
  "bugs": {
    "url": "https://github.com/zazuko/rdf-vocabularies/issues"
  },
  "license": "MIT",
  "files": [
    "*.js",
    "*.d.ts",
    "*.nq",
    "*.nt"
  ],
  "peerDependencies": {
    "@rdfjs/types": "^2.0.0"
  }
}
