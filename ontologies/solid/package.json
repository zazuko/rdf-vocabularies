  "scripts": {
    "fetch": "npx vocab-build datasets",
    "prebuild": "npx vocab-build module",
    "build": "tsc",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@vocabulary/builder": "^1.0.10"
  },
  "homepage": "https://prefix.zazuko.com/prefix/solid:",
  "vocabulary": {
    "prefix": "solid",
    "namespace": "http://www.w3.org/ns/solid/terms#"
  },
  "bugs": {
    "url": "https://github.com/zazuko/rdf-vocabularies/issues"
  },
  "license": "MIT",
  "files": [
    "*.js",
    "*.d.ts",
    "*.nq",
    "*.nt"
  ],
  "peerDependencies": {
    "@rdfjs/types": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zazuko/rdf-vocabularies",
    "directory": "ontologies/solid"
  }
}
